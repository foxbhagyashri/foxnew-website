# =========================================
# Foxaircomm - Security & SEO Rules
# =========================================

# Block hacked WordPress-style date-based URLs: /YYYY/MM/DD/*
RedirectMatch 410 ^/[0-9]{4}/[0-9]{2}/[0-9]{2}/.*$

# Enable rewrite engine
<IfModule mod_rewrite.c>
RewriteEngine On

# Force HTTPS and non-www (https://foxaircomm.com)
RewriteCond %{HTTPS} off [OR]
RewriteCond %{HTTP_HOST} ^www\. [NC]
RewriteRule ^(.*)$ https://foxaircomm.com/$1 [L,R=301]

</IfModule>

# Default directory index
DirectoryIndex index.php index.html